<!--信息-->
<div class="tui-reg-step-content  tui-partner-container   col-md-offset-2">
	<form action="" id="tui-reg-partner-form" class=" form-horizontal" role="form" name="partnerForm">
		<div class="form-group">
			<label for="Name" class="col-md-2 control-label">股东姓名：</label>
			<div class="col-md-6 ">
				<input type="text" name="Name" id="Name" value="" class="form-control" placeholder="" ng-model="parnters.Name" required="required" />
			</div>
			<div class="col-md-3" ng-show="partnerForm.Name.$dirty&&partnerForm.Name.$invalid">
				<span class="tui-tip">请输入姓名</span>
			</div>
		</div>
		<div class="form-group">
			<label for="EquityRatio" class="col-md-2 control-label">股权比率：</label>
			<div class="col-md-6 ">
				<input type="tel" name="" id="EquityRatio" value="" class="form-control" ng-model="parnters.EquityRatio" required="required" />
			</div>
			<div class="col-md-3" ng-show="partnerForm.EquityRatio.$dirty&&partnerForm.EquityRatio.$invalid">
				<span class="tui-tip">请输入股权比率</span>
			</div>
		</div>
		<div class="form-group">
			<label for="ProfitRatio" class="col-md-2 control-label">利润分配率：</label>
			<div class="col-md-6 ">
				<input type="tel" name="" id="ProfitRatio" value="" class="form-control" ng-model="parnters.ProfitRatio" required="required" />
			</div>
			<div class="col-md-3" ng-show="partnerForm.ProfitRatio.$dirty&&partnerForm.ProfitRatio.$invalid">
				<span class="tui-tip">请输入利润分配比率</span>
			</div>
		</div>
		<div class="form-group">
			<label for="Address" class="col-md-2 control-label">联系地址：</label>
			<div class="col-md-6 ">
				<input type="text" name="Address" id="Address" value="" class="form-control" ng-model="parnters.Address" />
			</div>
		</div>
		<div class="form-group">
			<label for="Telephone" class="col-md-2 control-label">手机号码：</label>
			<div class="col-md-6 ">
				<input type="tel" name="Telephone" id="Telephone" value="" class="form-control" ng-pattern="/^(0|86)?(13[0-9]|15[012356789]|17[678]|18[0-9]|14[57])[0-9]{8}$/" ng-model="parnters.Telephone" required="required" />
			</div>
			<div class="col-md-3" ng-show="partnerForm.Contact.$dirty&&partnerForm.Contact.$invalid">
				<span class="tui-tip">请输入正确的手机号</span>
			</div>
		</div>
		<div class="form-group">
			<label for="Account" class="col-md-2 control-label">用户名：</label>
			<div class="col-md-6 ">
				<input type="text" name="Account" id="Account" value="" class="form-control" ng-model="parnters.Account" />
			</div>
		</div>
		<div class="form-group">
			<label for="Pwd" class="col-md-2 control-label">登录密码：</label>
			<div class="col-md-6 ">
				<input type="password" name="Pwd" id="Pwd" value="" class="form-control" ng-model="parnters.Pwd" required="required" ng-keyup="meter()" />
				<p style="height: 25px; ">
					<span class="tui-pwd-bar ">
													<i class="tui-pwd-box ">弱</i>
													<i class="tui-pwd-box ">中</i>
													<i class="tui-pwd-box ">强</i>
												</span>
				</p>
			</div>
			<div class="col-md-2 " ng-show="partnerForm.Pwd.$dirty&&partnerForm.Pwd.$invalid">
				<span class="tui-tip">请输入密码</span>
			</div>
		</div>
		<div class="form-group">
			<label for="ConfigPwd" class="col-md-2 control-label">确认密码：</label>
			<div class="col-md-6 ">
				<input type="password"  id="ConfigPwd" value="" class="form-control" ng-model="parnters.ConfigPwd" required="required" ng-blur="configpwd()" />
			</div>
			<div class="col-md-3">
				<span class="tui-tip" ng-show="memberForm.Pwd.$valid&&memberForm.ConfigPwd.$dirty&&memberForm.ConfigPwd.$invalid">请确认密码</span>
				<span class="tui-tip" ng-show="memberForm.Pwd.$valid&&memberForm.ConfigPwd.$valid&&configpwdmsg">两次密码输入不一致</span>
			</div>
		</div>
		<div class="form-group">
			<label for="Code" class="col-md-2 control-label">验证码：</label>
			<div class="col-md-3 ">
				<input type="text" name="Code" class="form-control" placeholder="" ng-model="Code" required="required" />
			</div>
			<div class="col-md-3 ">
				<img src="images/vercode.png" alt="" />
			</div>
			<div class="col-md-2 " ng-show="partnerForm.Code.$dirty&&partnerForm.Code.$invalid">
				<span class="tui-tip">请输入验证码</span>
			</div>
		</div>
		<div class="form-group">
			<div class="col-md-6  col-md-offset-3">
				<input type="button" name=" " id=" " value="保存 " class=" tui-btn-default tui-btn-saveMember " ng-click="savePartners()" />
			</div>
		</div>
	</form>

</div>
