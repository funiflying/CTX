	<script src="lib/bootstrap/js/bootstrap-datetimepicker.js"></script>
	<script src="lib/bootstrap/js/locales/bootstrap-datetimepicker.zh-CN.js"></script>
		<script type="text/javascript">
			$(function() {
                $('.form_datetime').datetimepicker({
                    language: 'zh-CN',
                    format: "yyyy-mm-dd",
                    autoclose: true,
                    todayBtn: true,
                    viewSelect: 'month',
                    minView: 2
                });
	})
		</script>
	