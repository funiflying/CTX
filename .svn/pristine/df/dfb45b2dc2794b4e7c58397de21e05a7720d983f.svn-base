.search-wrap {
	width: 100%;
	height: 1px;
	position: absolute;
	top: 0;
	left: 0;
}
.search-wrap .main-box {
	margin: 10px auto;
}
.search-layout {
	width: 280px;
	height: 336px;
	padding: 15px;
	float: left;
	/*filter: alpha(opacity=65); opacity:0.65;*/
	
	overflow: hidden;
	background: #FFF;
	border-bottom: 2px solid #d6004b;
}
.search-layout .search-main {
	position: relative;
}
.search-main input {
	width: 250px;
	height: 35px;
	line-height: 35px;
	padding: 0 10px;
	background: #fff;
	border: 1px solid #ccc;
	color: #B2B2B2;
	-webkit-writing-mode: horizontal-tb;
}
.search-submit {
	border: 0;
	width: 36px;
	height: 35px;
	right: 0px;
	top: 0px;
	position: absolute;
	/*background-position: -217px -63px;*/
	
	background-color: transparent;
	cursor: default;
	background: url(../images/search-submit.gif) no-repeat;
}
.d-index-brand {
	width: 100%;
	position: relative;
	float: left;
	margin: 10px 0px 10px 0px;
}
.d-index-brand .d-title {
	font-size: 16px;
	color: #ff6a00;
	float: left;
	width: 100%;
}
.d-index-brand .d-more {
	font-size: 14px;
	color: #ff6a00;
	float: right;
}
.d-index-brand .d-content {
	width: 100%;
	text-align: center;
}
.d-index-brand .d-content a {
	width: 47px;
	padding-top: 5px;
	padding-bottom: 5px;
	display: inline-block;
}
.d-service {
	height: auto;
	line-height: 150px;
	font-size: 14px;
}
.d-service img {
	padding-right: 30px;
	border-right: 1px solid #d9d9d9;
	margin-right: 30px;
}
.d-service span {
	width: 85px;
	height: 85px;
	text-align: left;
}
.index-cat {
	width: 300px;
	height: 370px;
	float: left;
	filter: alpha(opacity=65);
	opacity: 0.65;
	overflow: hidden;
	background: #FFF;
	border-bottom: 2px solid #d6004b;
}
