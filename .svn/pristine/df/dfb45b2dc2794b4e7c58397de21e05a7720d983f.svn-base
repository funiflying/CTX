.index-info {
	width: 100%;
	display: inline-block;
}
.index-info li {
	width: 25%;
	float: left;
	padding-top: 20px;
	padding-bottom: 20px;
}
.index-info li span {
	margin-left: 55px;
	overflow: hidden;
	font-size: 16px;
	line-height: 50px;
	display: block;
	color: #666;
}
.index-box-bg {
	background: #f5f5f5;
	display: inline-block;
	padding-bottom: 15px;
	padding-top: 15px;
}
.index-list-title {
	height: 45px;
	background: #fff;
	width: 100%;
	overflow: hidden;
}
.index-title-wrap {
	display: inline-block;
	line-height: 45px;
	height: 45px;
	background: #fff;
	width: 100%;
	overflow: hidden;
}
.index-title-wrap .title-left {
	float: left;
	font-size: 16px;
}
.index-title-wrap .title-left strong {
	font-size: 16px;
	margin-left: 15px;
}
.index-title-wrap .title-left a {
	padding: 0px 15px 0px 15px;
	border-right: 1px solid #666;
	margin-right: 10px;
}

.index-title-wrap .title-right {
	float: right;
	font-size: 16px;
	margin-right: 15px;
}
.index-title-wrap .on {
	color: #2AC845;
}
.retlist {
	width: 100%;
	margin-bottom: 25px;
	position: relative;
	float: left;
}
.list-ul {
	margin-right: -10px;
}
/*.list-ul li{height: 315px;width: 292.5px;float: left;margin-right: 10px;_display: inline;position: relative;z-index: 2;}*/

.list-ul li {
	width: 292.5px;
	height: 300px;
	float: left;
	margin-top: 15px;
	margin-right: 10px;
	_display: inline;
	position: relative;
	z-index: 2;
	overflow: hidden;
	background: #FFF;
}
.list-ul li:hover {
	box-shadow: 0 3px 3px #b2b2b2;
}
.list-ul li img {
	width: 292.5px;
	height: auto;
	overflow: hidden;
}
.list-ul li p {
	width: 100%;
	padding: 6px 10px 6px 10px;
}
.list-ul li p span {
	padding-right: 10px;
}
.list-ul li .title {
	font-size: 14px;
	letter-spacing: normal;
	height: 25px;
	width: 100%;
	word-spacing: normal;
	overflow: hidden;
}
.list-ul li .price {
	font-size: 16px;
	color: #f60;
}
.d-i-more {
	width: 100%;
	margin-bottom: 15px;
	position: relative;
	float: left;
	text-align: center;
}
.d-i-more a {
	color: #fff;
	background: #d6004b;
	padding: 10px 25px 10px 25px;
	-moz-border-radius: 5px;
	-webkit-border-radius: 5px;
	border-radius: 5px;
}
.d-i-more a:hover {
	text-decoration: none;
	background: #920537;
}
/*首页banner上的筛选框*/

