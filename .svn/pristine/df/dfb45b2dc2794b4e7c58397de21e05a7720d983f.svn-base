table {
	border-collapse: collapse;
	border-spacing: 0;
}
tr {
	display: table-row;
	vertical-align: inherit;
	border-color: inherit;
}
.ppmore table td {
	vertical-align: top;
	padding: 5px 0;
	line-height: 18px;
	/*overflow: hidden;*/
}
.zm {
