@charset "utf-8";
.index-list {
	display: inline-block;
}
.index-list ul {
	width: 100%;
	height: auto;
	position: relative;
	float: left;
}
.index-list li {
	width: 25%;
	height: auto;
	float: left;
}
.index-list li div {
	float: left;
}
.index-grid {
	width: 295px;
	height: 250px;
	z-index: 1;
	text-align: center;
	padding-top: 5px;
	border: 1px solid #666666;
	overflow: hidden;
	float: left;
	background: #FFF;
}
.index-grid img {
	width: 96%;
}
