<div class="row " style="height: 430px;padding: 50px 0 ;">
	<div class="col-md-3 text-center">
		<img src="images/1.jpg" style="width:130px;height:180px;margin-top: 15px;" />
        
	</div>
	<div class="col-md-9">
		<table border="0" cellspacing="0" cellpadding="0" class="tui-audit-members-tab">
			<tr>
				<th>姓名：</th><td ng-bind="member.Name"></td>
				<th>性别：</th><td>男</td>
			</tr>
			<tr>
				<th>手机号：</th><td ng-bind="member.Contact"></td>
