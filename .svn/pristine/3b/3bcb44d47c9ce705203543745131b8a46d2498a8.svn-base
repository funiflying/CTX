/***************
 *
 *******index
 *
 **************/
body{
    font-size: 14px;
}
a:hover{
    text-decoration: none;
}
.tui-oa-navbar{
    background-color: #363f45;
    height: 60px;
    line-height: 60px;
}

.tui-oa-bar {
    background: url(../images/logo.png) left  center  no-repeat;
    height: 100%;
    text-align: right;
}
.tui-oa-bar i{
    color: #fff;
    font-size: 16px;
    margin: 0 10px;
}
.tui-oa-bar span.badge{
    position: absolute;
    right: -10px;
    top: -10px;
    width: 16px;
    height: 16px;
    line-height: 16px;
    font-size: 10px;
    background: #fe5621;
    font-weight: normal;
    border-radius: 50%;
    padding: 0;
}
.tui-oa-sidebar{
    height: 100%;
    background: #f5f5f5;
    text-align: center;
    padding: 30px 0;
}
.tui-oa-aside{
  

}
.tui-oa-aside a{
    margin: 0 10px;
    color: #333333;
}
.tui-admim-headIcon{
    width: 125px;
    height: 125px;
}
.tui-oa-name {
    color:#999999;
    line-height: 30px;
}

.tui-oa-aside-nav {
    padding: 30px 0;
    font-size: 16px;
    color: #4c4c4c;
}

.tui-oa-asidenav-item {
    min-height: 55px;
    line-height: 50px;
}
.tui-oa-asidenav-item >ul{
    font-size: 14px;
}
.tui-oa-asidenav-item a:hover,
.tui-oa-asidenav-item-active{
    background: #fff;
}
.tui-oa-asidenav-item a{
    display: block;
    outline: none;
}
.tui-oa-asidenav-item a:hover{
	color: #fe5621;
}
.tui-oa-personcenter{
    padding-top: 30px;
    padding-bottom: 30px;
}
.tui-oa-h2 {
    font-size: 16px;
    font-weight: bold;
    color: #333;
}
.tui-oa-h2+a {
    font-size: 12px;
    margin-left: 10px;
}

.tui-oa-personinfo {
    width: 50%;
    height: 60px;
    vertical-align: center;
}
.tui-oa-progressbar
{
    width: 60%;
    padding-top:10px;
}
.tui-oa-progressbar .progress{
    display: inline-block;
    width: 90%;
}

.tui-oa-coin {
    width: 280px;
    height: 170px;
    background: #f5f5f5;
    padding: 10px;
}

.tui-oa-icon-count {
    height: 90px;
    background-color: #fff;
    background-image: url(../images/coin-count.png);
    background-position: 20px center;
    background-repeat: no-repeat;
    position: relative;
}
.tui-oa-location {
    height: 45px;
    line-height: 45px;
}
/***************
 *
 *******orders
 *
 **************/
.tui-oa-order-item {
    height: 210px;
    border: 1px solid #e2e2e2;
    margin-bottom: 10px;
}
.tui-oa-order-caption>div{
    height: 40px;
    line-height: 40px;
    background: #f0f0f0;
    text-align: center;
}
.tui-oa-order-body>div{
    height: 170px;
    line-height: 170px;
    text-align: center;
    color: #333333;
}
.tui-oa-order-body>div h2{
    font-size: 18px;
    color: #333333;
}
.tui-oa-order-body>div p strong{
    font-size: 18px;
    font-weight: bold;
    color: #ff6a00;
}
.tui-oa-order-body>div p label{
    color: #ff6a00;
}
.tui-oa-order-body>div img{
   width: 190px;
   height: 150px;
}
.tui-oa-order-body>div a{
   color: #3250be;
    vertical-align:middle;
}
.tui-oa-order-body>div.tui-order-car{
    padding-top: 70px;
    line-height: 40px;
}

.tui-oa-down{
	font-size: 12px;
	color: #aaaaaa;	
	text-align: right;
}
.tui-oa-order-table
{
	color: #000000;
	font-size:16px ;
	line-height: 50px;
	border: none;
	margin: 20px 0;
}
.tui-oa-order-table td
{
	border: none;
}
/***************
 *
 *******审核
 *
 **************/
.tui-audit-tab{
	width: 100%;
	text-align: center;
	vertical-align: middle;
	border: 1px solid #eeeeee;
}
.tui-audit-tab thead td{
	font-size: 18px;
	color: #333333;
	background: #f5f5f5;
	height: 40px;
	border-right: 1px solid #fff;
}
.tui-audit-tab tbody td{
	font-size: 16px;
	color: #888888;
	background: #ffffff;
	height: 120px;
	border: 1px solid #eeeeee;
}
.tui-audit-members-tab{
	border: none;
	font-size: 14px;
	color: #1c1c1c;
	width: 100%;	
	line-height: 45px;
	font-family: "微软雅黑";
	}
.tui-audit-members-tab th{
	font-size: 14px;
	color: #666666;
    text-align: right;
}
.tui-audit-members-tab td{
	text-align: left;
	padding-left: 15px;
	font-size: 14px;
	color: #1c1c1c;
}












